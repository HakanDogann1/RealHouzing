@model UpdateContactChannelViewModel
@{
    ViewData["Title"] = "UpdateContactChannel";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
    <input type="hidden" asp-for="ContactChannelID"/>
    <br />
    <label>Başlık:</label>
    <input type="text" class="form-control" asp-for="ContactChannelTitle" />
    <br />
    <label>İçerik:</label>
    <input type="text" class="form-control" asp-for="ContactChannelDescription" />
    <br />
    <label>Resim:</label>
    <input type="text" class="form-control" asp-for="Icon" />
    <br />
    <button class="btn btn-success">Kaydet</button>
</form>

