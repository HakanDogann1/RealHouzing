@model List<AboutService1ListViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Başlık</th>
        <th>İçerik</th>
        <th>Resim</th>
        <th>Güncelle</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AboutService1ID</th>
            <td>@item.AboutService1Title</td>
            <td>@item.AboutService1Description</td>
            <td>@item.ImageUrl</td>
            <td><a href="/AboutService1/UpdateAboutService1/@item.AboutService1ID" class="btn btn-outline-warning">Güncelle</a></td>
        </tr>
    }

</table>
<br />



